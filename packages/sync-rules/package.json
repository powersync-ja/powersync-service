{
  "name": "@powersync/service-sync-rules",
  "repository": "https://github.com/powersync-ja/powersync-service",
  "version": "0.31.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "FSL-1.1-ALv2",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*",
    "schema/*"
  ],
  "type": "module",
  "scripts": {
    "clean": "rm -r ./dist && tsc -b --clean",
    "build:tsc": "tsc -b",
    "build": "pnpm build:tsc && pnpm run build:extra",
    "build:extra": "node scripts/compile-schema.js",
    "build:tests": "tsc -b test/tsconfig.json",
    "test": "vitest",
    "generate:grammar-flat": "tsx scripts/generate-grammar-docs.ts --mode flat"
  },
  "dependencies": {
    "@powersync/service-jsonbig": "workspace:^",
    "@syncpoint/wkx": "^0.5.0",
    "ajv": "^8.12.0",
    "pgsql-ast-parser": "^11.1.0",
    "uuid": "^11.1.0",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.16.2",
    "ebnf": "^1.9.1",
    "railroad-diagrams": "^1.0.0",
    "tsx": "^4.21.0",
    "vitest": "catalog:"
  }
}
