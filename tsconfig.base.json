{
  "compilerOptions": {
    "lib": ["ES2024"],
    "target": "ES2024",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": true,
    "composite": true,
    "declaration": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "useUnknownInCatchVariables": false
  }
}
